//to red
var image = new SimpleImage("chapel.png");
for (var pixel of image.values()){
    if(pixel.getRed()){
        pixel.setRed(255);
    }
    else{
        pixel.setRed(pixel.getRed());
    }
}

print(image);

//to green
var image = new SimpleImage("chapel.png");
for (var pixel of image.values()){
    pixel.setRed(0);
}

print(image);
